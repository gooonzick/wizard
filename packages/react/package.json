{
	"name": "@gooonzick/wizard-react",
	"version": "0.0.1",
	"description": "React integration for Wizard framework",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "vite build",
		"typecheck": "tsc --build --verbose",
		"test": "vitest --run",
		"test:watch": "vitest --watch",
		"lint": "biome check .",
		"lint:fix": "biome check . --write"
	},
	"peerDependencies": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"dependencies": {
		"@gooonzick/wizard-core": "workspace:*"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.6",
		"@vitejs/plugin-react": "^5.1.1",
		"jsdom": "^27.2.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"typescript": "^5.9.3",
		"vite": "npm:rolldown-vite@^7.2.5",
		"vite-plugin-dts": "^4.5.4",
		"vitest": "^4.0.13"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gooonzick/wizard",
		"directory": "packages/react"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"wizard",
		"react",
		"hook",
		"form",
		"multi-step"
	]
}
